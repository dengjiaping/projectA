<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
<com.zhy.autolayout.AutoRelativeLayout
    android:layout_width="702px"
    android:layout_height="88px"
    >
    <com.hzease.tomeet.widget.CircleImageView
        android:layout_centerVertical="true"
        android:id="@+id/civ_finfish_icon"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_marginLeft="40px"
        />
    <TextView
        android:id="@+id/tv_finish_name_fmt"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/civ_finfish_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20px"
        android:maxLength="12"
        android:textSize="30px"
        android:textColor="#333"
        />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="88px"
        android:layout_marginRight="40px"
        android:gravity="center_vertical"
        >
        <TextView
            android:id="@+id/tv_finish_point_fmt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textSize="30px"
            android:textColor="#333"
            android:layout_marginRight="40px"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/currency"
            />
        <TextView
            android:layout_marginLeft="5px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333"
            android:textSize="30px"
            android:id="@+id/tv_finsh_currency_fmt"
            />
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoRelativeLayout>