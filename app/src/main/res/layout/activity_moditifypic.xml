<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_modifitypic_name_aty"
            style="@style/Fragment_Head"
            android:text="韩梅梅"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333"
            android:text="编辑"
            android:textSize="32px"
            android:layout_alignBaseline="@+id/tv_modifitypic_name_aty"
            android:layout_alignParentRight="true"
            android:layout_marginRight="24px"
            android:layout_marginEnd="28px"
            android:id="@+id/tv_modifitypic_moditity_aty" />
    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:layout_width="500px"
            android:layout_height="500px"
            android:src="@drawable/upload_photo_big"
            android:id="@+id/imageView8" />
        <ImageView
            android:layout_width="250px"
            android:layout_height="250px"
            android:src="@drawable/upload_photo_small"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/imageView8"
            android:id="@+id/imageView9" />
        <ImageView
            android:layout_width="250px"
            android:layout_height="250px"
            android:src="@drawable/upload_photo_small"
            android:layout_below="@+id/imageView9"
            android:layout_toRightOf="@+id/imageView8"
            />
        <ImageView
            android:layout_width="250px"
            android:layout_height="250px"
            android:src="@drawable/upload_photo_small"
            android:layout_below="@+id/imageView8"
            android:id="@+id/imageView10" />
        <ImageView
            android:layout_width="250px"
            android:layout_height="250px"
            android:src="@drawable/upload_photo_small"
            android:layout_below="@+id/imageView8"
            android:layout_toRightOf="@+id/imageView10"
            android:layout_toEndOf="@+id/imageView10" />
        <ImageView
            android:layout_width="250px"
            android:layout_height="250px"
            android:src="@drawable/upload_photo_small"
            android:layout_below="@+id/imageView8"
            android:layout_toRightOf="@+id/imageView8"
            />
    </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoLinearLayout>